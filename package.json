{
  "version": "0.1.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "clear; tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "ided",
  "author": "Dominic Victoria <dominicvictoriadev@gmail.com> (https://dominicvictoria.com/)",
  "module": "dist/ided.esm.js",
  "size-limit": [
    {
      "path": "dist/ided.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/ided.esm.js",
      "limit": "10 KB"
    }
  ],
  "keywords": [
    "id",
    "key",
    "list",
    "array",
    "react",
    "unique",
    "order"
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^6.0.4",
    "husky": "^7.0.4",
    "size-limit": "^6.0.4",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^3.9.10"
  },
  "dependencies": {
    "@types/uuid": "^8.3.3",
    "uuid": "^3.4.0"
  }
}
